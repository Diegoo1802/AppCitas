<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/img_perfil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="73dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/txt_nombrePerfil"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:text="TRAER NOMBRE"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="314dp" />

    <TextView
        android:id="@+id/txt_passPerfil"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:text="+++++"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="389dp" />

    <TextView
        android:id="@+id/btn_ubi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ubicacion"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="465dp" />

    <TextView
        android:id="@+id/btn_terapeuta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Terapeuta"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="509dp" />

    <TextView
        android:id="@+id/btn_hor"
        android:layout_width="63dp"
        android:layout_height="21dp"
        android:text="Horario"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="546dp" />

    <TextView
        android:id="@+id/btn_telf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefono"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="585dp" />

</androidx.constraintlayout.widget.ConstraintLayout>